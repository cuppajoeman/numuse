from typing import List, Tuple
from fractions import Fraction

class Music:
    """Represents notes that are played over time"""
    def __init__(self, music_data: List[Tuple[str, List[Fraction]]]):
        self.music_data = music_data

    def generate_song_points():
        duration = 0
        song_points = []
        rhythm_lines = []
        for measure in song:
            notes, rhythms = measure
            notes = get_intervals(notes)
            note_to_rhythm = zip(notes, rhythms)
            for n, r in note_to_rhythm:
                if n is not None:
                    song_points.append((duration, n))
                    rhythm_lines.append(r)
                duration += r
